    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
  </padding>
  <children>
    <GridPane hgap="10.0" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.vgrow="NEVER">
      <children>
 		<Button fx:id="printPdfBtn" onMouseClicked="#onPrintData" styleClass="btn, btn-sm, btn-success" text="帳票出力(F9)" GridPane.columnIndex="0" GridPane.rowIndex="0" />
