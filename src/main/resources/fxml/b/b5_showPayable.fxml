      <GridPane.margin>
        <Insets bottom="5.0" left="0.0" right="0.0" top="0.0" />
      </GridPane.margin>
    </GridPane>
    
    <TableView fx:id="payableDataTable" editable="true" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
	  <columns>
	    <TableColumn fx:id="torihikisakiAndDate" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" sortable="false" text="取引先(最終取引日)" />
	    <TableColumn fx:id="carryForwardAmountMoney" maxWidth="-1.0" minWidth="50.0" prefWidth="-1.0" sortable="false" text="前期繰越" style="-fx-alignment: center-right;" />
	    <TableColumn fx:id="month1" maxWidth="-1.0" minWidth="50.0" prefWidth="-1.0" sortable="false" text="YYYY/MM">
	        <columns>
                <TableColumn fx:id="depPay1" minWidth="50.0" prefWidth="-1.0" sortable="false" text="支払" style="-fx-alignment: center-right;" />
                <TableColumn fx:id="request1" minWidth="50.0" prefWidth="-1.0" sortable="false" text="請求" style="-fx-alignment: center-right;" />
                <TableColumn fx:id="balance1" minWidth="50.0" prefWidth="-1.0" sortable="false" text="残高" style="-fx-alignment: center-right;" />
            </columns>
        </TableColumn>
        <TableColumn fx:id="month2" maxWidth="-1.0" minWidth="50.0" prefWidth="-1.0" sortable="false" text="YYYY/MM">
	        <columns>
                <TableColumn fx:id="depPay2" minWidth="50.0" prefWidth="-1.0" sortable="false" text="支払" style="-fx-alignment: center-right;" />
