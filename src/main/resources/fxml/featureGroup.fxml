            <MenuItem mnemonicParsing="false" onAction="#onMenuA3" text="年度決算" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="会計帳票">
          <items>      
            <MenuItem mnemonicParsing="false" onAction="#onMenuB1" text="合計残高試算表" />
            <MenuItem mnemonicParsing="false" onAction="#onMenuB2" text="総勘定元帳" />
            <SeparatorMenuItem mnemonicParsing="false" />
