<VBox fx:id="mainVBox" prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.busicomjp.sapp.controller.FeatureGroupController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
