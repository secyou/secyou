          <items>
            <MenuItem mnemonicParsing="false" onAction="#onMenuA1" text="仕訳登録" />
            <MenuItem mnemonicParsing="false" onAction="#onMenuA2" text="仕訳帳" />
            <SeparatorMenuItem mnemonicParsing="false" />
