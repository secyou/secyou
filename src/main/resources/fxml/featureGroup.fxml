          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="各種設定">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#onMenuD1" text="会社情報管理" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#onMenuD2" text="取引先管理" />
            <MenuItem mnemonicParsing="false" onAction="#onMenuD3" text="勘定科目管理" />
            <MenuItem mnemonicParsing="false" onAction="#onMenuD4" text="摘要管理" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="会社切替">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#onMenuSwitch" text="会社切替" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="終了">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#onMenuExit" text="終了" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <GridPane styleClass="page" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
          <children>
            <TabPane fx:id="appTab" styleClass="tab-pane-app" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.vgrow="ALWAYS">
               <tabs>
               

               </tabs>
			</TabPane>
            
          </children>
          <columnConstraints>
            <ColumnConstraints />
                  <ColumnConstraints />
